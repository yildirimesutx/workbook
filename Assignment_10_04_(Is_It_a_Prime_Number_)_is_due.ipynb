      "source": [
        "n = int(input(\"enter a number: \"))\n",
        "\n",
        "counter = 0\n",
        "\n",
        "for i in range(1, n+1):\n",
        "   if n % i == 0 :\n",
        "      counter += 1\n",
        "\n",
        "if (n==0) or (n==1) or (counter >= 3) :\n",
        "    print(n, \"is not a Prime number\")\n",
        "\n",
        "else :\n",
        "  print(n, \"is a Prime number\")"
      ],
